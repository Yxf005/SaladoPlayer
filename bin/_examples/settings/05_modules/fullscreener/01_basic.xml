<?xml version="1.0" encoding="utf-8" ?>
<!--
	Module allows adding and customizing simple button for entering and exiting fullscreen state
	it also inidicates current state of panorama window.
	
	See: [../../_manual/index.html?module_fullscreener.html]
-->
<SaladoPlayer>
	<global debug="true">
		<trace align="horizontal:left"/>
	</global>
	<panoramas>
		<panorama id="checkerRed" path="~tours/checker/dz_red/red_f.xml"/>
	</panoramas>
	<modules>
		<FullScreener path="~modules/fullscreener/FullScreener-1.1.swf">
			<settings path="~modules/fullscreener/skins/button_thick_black_50x50.png"/>
		</FullScreener>
	</modules>
</SaladoPlayer>