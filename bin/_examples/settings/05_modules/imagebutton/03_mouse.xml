<?xml version="1.0" encoding="utf-8"?>
<!-- 
	Assign mouse events on interaction with each button. You can display content
	when mouse is hovering over button with other modules, like InfoBubble.
	
	../../_manual/index.html?module_imagebutton.html
-->
<SaladoPlayer>
	<global debug="true">
		<trace open="true"/>
	</global>
	<panoramas>
		<panorama id="checkerGreen" path="~panos/checker/dz_green/green_f.xml"/>
	</panoramas>
	<modules>
		<ImageButton path="~modules/imagebutton/ImageButton-1.3.swf">
			<button id="logo1" path="~media/images/misc/panozona_logo.png" mouse="onOver:printOver,onOut:printOut"/>
		</ImageButton>
	</modules>
	<actions>
		<action id="printOver" content="SaladoPlayer.print(over)"/>
		<action id="printOut" content="SaladoPlayer.print(out)"/>
		<action id="printClick" content="SaladoPlayer.print(click)"/>
	</actions>
</SaladoPlayer>