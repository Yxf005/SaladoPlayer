<?xml version="1.0" encoding="utf-8" ?>
<SaladoPlayer>
	<global debug="false">
		<control autorotation="delay:600"/>
		<panoramas firstPanorama="front"/>
	</global>
	<panoramas>
		<panorama id="front" path="~tours/olawa/dz_front/front_f.xml" camera="pan:NaN" direction="198">
			<swf id="front_to_interior" location="pan:3.6,tilt:3.6" path="~spots/GrowingSpot-1.0.swf" target="interior">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="front_to_p2b" location="pan:65.4,tilt:5.3" path="~spots/GrowingSpot-1.0.swf" target="p2b">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
		<panorama id="p1" path="~tours/olawa/dz_p1/p1_f.xml" camera="pan:NaN" direction="310">
			<swf id="p1_to_interior" location="pan:36.3,tilt:-13.1" path="~spots/GrowingSpot-1.0.swf" target="interior">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p1_to_passage" location="pan:175.9,tilt:-1.11" path="~spots/GrowingSpot-1.0.swf" target="passage">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p1_to_p2a" location="pan:-96.7,tilt:-3.24" path="~spots/GrowingSpot-1.0.swf" target="p2a">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p1_to_p2b" location="pan:-25,tilt:-0.5" path="~spots/GrowingSpot-1.0.swf" target="p2b">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
		<panorama id="p2a" path="~tours/olawa/dz_p2a/p2a_f.xml" camera="pan:NaN" direction="310">
			<swf id="p2a_to_passage" location="pan:174,tilt:0" path="~spots/GrowingSpot-1.0.swf" target="passage">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p2a_to_p2b" location="pan:2,tilt:0" path="~spots/GrowingSpot-1.0.swf" target="p2b">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p2a_to_p1" location="pan:85,tilt:-2.5" path="~spots/GrowingSpot-1.0.swf" target="p1">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
		<panorama id="p2b" path="~tours/olawa/dz_p2b/p2b_f.xml" camera="pan:NaN" direction="130">
			<swf id="p2b_to_front" location="pan:-74,tilt:0" path="~spots/GrowingSpot-1.0.swf" target="front">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p2b_to_p1" location="pan:-27,tilt:0.2" path="~spots/GrowingSpot-1.0.swf" target="p1">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="p2b_to_p2a" location="pan:-2,tilt:0.5" path="~spots/GrowingSpot-1.0.swf" target="p2a">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
		<panorama id="interior" path="~tours/olawa/dz_interior/interior_f.xml" camera="pan:NaN" direction="130">
			<swf id="interior_to_front" location="pan:-79.2,tilt:-6.21" path="~spots/GrowingSpot-1.0.swf" target="front">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="interior_to_p1" location="pan:101.5,tilt:-5.29" path="~spots/GrowingSpot-1.0.swf" target="p1">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
		<panorama id="passage" path="~tours/olawa/dz_passage/passage_f.xml" camera="pan:NaN" direction="150">
			<swf id="passage_to_p1" location="pan:-126,tilt:0" path="~spots/GrowingSpot-1.0.swf" target="p1">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
			<swf id="passage_to_p2a" location="pan:151,tilt:16.5" path="~spots/GrowingSpot-1.0.swf" target="p2a">
				<settings path="~tours/olawa/_media/images/spots/go.png"/>
			</swf>
		</panorama>
	</panoramas>
	<modules>
		<InfoBubble path="~modules/infobubble/InfoBubble-1.3.3.swf">
			<settings alpha="1.0"/>
			<bubbles>
				<image id="bFront" path="~tours/olawa/dz_front/_thumbs/200x100.jpg"/>
				<image id="bP1" path="~tours/olawa/dz_p1/_thumbs/200x100.jpg"/>
				<image id="bP2a" path="~tours/olawa/dz_p2a/_thumbs/200x100.jpg"/>
				<image id="bP2b" path="~tours/olawa/dz_p2b/_thumbs/200x100.jpg"/>
				<image id="bInterior" path="~tours/olawa/dz_interior/_thumbs/200x100.jpg"/>
				<image id="bPassage" path="~tours/olawa/dz_passage/_thumbs/200x100.jpg"/>
			</bubbles>
		</InfoBubble>
		<ImageButton path="~modules/imagebutton/ImageButton-1.3.swf">
			<button id="help" path="~tours/_media/images/misc/help.png" action="toggleHelp">
				<window align="vertical:middle,horizontal:center" open="true" onOpen="onOpenHelp" onClose="onCloseHelp"/>
			</button>
			<button id="buttonMap" path="~tours/_media/images/buttons/dark_right_open.png" action="mapOpen">
				<window align="vertical:middle,horizontal:right" move="horizontal:0,vertical:0" openTween="time:0" closeTween="time:0"/>
			</button>
			<button id="buttonMenu" path="~tours/_media/images/buttons/dark_left_open.png" action="menuOpen">
				<window align="vertical:middle,horizontal:left" move="horizontal:0,vertical:0" openTween="time:0" closeTween="time:0"/>
			</button>
		</ImageButton>
		<ImageMap path="~modules/imagemap/ImageMap-1.4.3.swf">
			<window open="false" onOpen="mapOpened" onClose="mapClosed" maxSize="width:250,height:2000" minSize="width:250,height:200" margin="right:0,top:0,bottom:0" />
			<viewer path="~modules/imagemap/skins/navigation_black_20x20.png" style="alpha:0.75"/>
			<close path="~tours/_media/images/buttons/dark_right_close.png" align="vertical:middle,horizontal:left" move="horizontal:-40"/>
			<maps>
				<map id="map1" path="~tours/olawa/_media/images/maps/map.jpg">
					<waypoints path="~modules/imagemap/skins/waypoints_bubble_45x45.png" move="horizontal:6,vertical:-22">
						<waypoint target="front" position="x:405,y:110" mouse="onOver:showBubble_bFront,onOut:hideBubble"/>
						<waypoint target="p1" position="x:350,y:250" mouse="onOver:showBubble_bP1,onOut:hideBubble"/>
						<waypoint target="p2a" position="x:300,y:280" mouse="onOver:showBubble_bP2a,onOut:hideBubble"/>
						<waypoint target="p2b" position="x:160,y:160" mouse="onOver:showBubble_bP2b,onOut:hideBubble"/>
						<waypoint target="interior" position="x:360,y:200" mouse="onOver:showBubble_bInterior,onOut:hideBubble"/>
						<waypoint target="passage" position="x:390,y:350" mouse="onOver:showBubble_bPassage,onOut:hideBubble"/>
					</waypoints>
				</map>
			</maps>
		</ImageMap>
		<MenuScroller path="~modules/menuscroller/MenuScroller-1.3.3.swf">
			<close path="~tours/_media/images/buttons/dark_left_close.png" align="vertical:middle" move="horizontal:40"/>
			<window open="false" onOpen="menuOpened" onClose="menuClosed" openTween="time:0.5" closeTween="time:0.5"/>
			<groups>
				<group id="g1">
					<element target="front" path="~tours/olawa/dz_front/_thumbs/200x100.jpg"/>
					<element target="p1" path="~tours/olawa/dz_p1/_thumbs/200x100.jpg"/>
					<element target="p2a" path="~tours/olawa/dz_p2a/_thumbs/200x100.jpg"/>
					<element target="p2b" path="~tours/olawa/dz_p2b/_thumbs/200x100.jpg"/>
					<element target="interior" path="~tours/olawa/dz_interior/_thumbs/200x100.jpg"/>
					<element target="passage" path="~tours/olawa/dz_passage/_thumbs/200x100.jpg"/>
				</group>
			</groups>
		</MenuScroller>
		<ButtonBar path="~modules/buttonbar/ButtonBar-1.3.swf">
			<window align="horizontal:center"/>
			<buttons path="~modules/buttonbar/skins/buttons_dark_40x40.png">
				<button name="out"/>
				<button name="in"/>
				<button name="drag"/>
				<button name="autorotation" />
				<button name="fullscreen" />
				<extraButton name="e" action="toggleHelp"/>
			</buttons>
		</ButtonBar>
		<MouseCursor path="~modules/mousecursor/MouseCursor-1.3.swf">
			<settings path="~modules/mousecursor/skins/cursors_21x21.png" showLine="true"/>
		</MouseCursor>
	</modules>
	<actions>
		<action id="mapOpen" content="ImageMap.setOpen(true)"/>
		<action id="mapClose" content="ImageMap.setOpen(false)"/>
		<action id="mapOpened" content="ImageButton.setOpen(buttonMap,false)"/>
		<action id="mapClosed" content="SaladoPlayer.waitThen(0.5,openButtonMap)"/>
		<action id="openButtonMap" content="ImageButton.setOpen(buttonMap,true)"/>
		
		<action id="menuOpen" content="MenuScroller.setOpen(true)"/>
		<action id="menuClose" content="MenuScroller.setOpen(false)"/>
		<action id="menuOpened" content="ImageButton.setOpen(buttonMenu,false)"/>
		<action id="menuClosed" content="SaladoPlayer.waitThen(0.5,openButtonMenu)"/>
		<action id="openButtonMenu" content="ImageButton.setOpen(buttonMenu,true)"/>
		
		<action id="hideBubble" content="InfoBubble.hide()"/>
		<action id="showBubble_bFront" content="InfoBubble.show(bFront)"/>
		<action id="showBubble_bP1" content="InfoBubble.show(bP1)"/>
		<action id="showBubble_bP2a" content="InfoBubble.show(bP2a)"/>
		<action id="showBubble_bP2b" content="InfoBubble.show(bP2b)"/>
		<action id="showBubble_bInterior" content="InfoBubble.show(bInterior)"/>
		<action id="showBubble_bPassage" content="InfoBubble.show(bPassage)"/>
		
		<action id="toggleHelp" content="ImageButton.toggleOpen(help)"/>
		<action id="onOpenHelp" content="ButtonBar.setActive(e,true)"/>
		<action id="onCloseHelp" content="ButtonBar.setActive(e,false)"/>
	</actions>
</SaladoPlayer>